{
  "name": "ecoscript",
  "version": "0.2.0",
  "description": "Simple, YAML-configurable natural ecosystem simulator",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dustinrohde/ecoscript.git"
  },
  "main": "lib/main.js",
  "bin": "./bin/ecoscript.js",
  "directories": {
    "test": "test"
  },
  "author": "Dustin Rohde",
  "license": "GPL-3.0",
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-transform-object-rest-spread": "6.5.0",
    "babel-preset-es2015": "6.5.0",
    "babel-tape-runner": "2.0.1",
    "docco": "0.7.0",
    "eslint": "2.4.0",
    "faucet": "0.0.1",
    "rimraf": "2.5.2",
    "sinon": "1.17.3",
    "tape": "4.5.1"
  },
  "dependencies": {
    "babel-polyfill": "6.5.0",
    "easystarjs": "0.2.3",
    "js-yaml": "3.5.3",
    "lodash": "4.3.0",
    "minimist": "1.2.0",
    "source-map-support": "0.4.0",
    "stampit": "2.1.1"
  },
  "babel": {
    "plugins": [
      "transform-object-rest-spread",
      "add-module-exports"
    ],
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build",
    "docs": "docco examples/*.yaml src/*.js",
    "clean": "rimraf lib/*",
    "build": "babel src/ -d lib/",
    "build-dev": "npm run build -- --source-maps inline",
    "watch": "npm run build-dev -- --watch",
    "test": "babel-tape-runner test/**/*.js | faucet",
    "lint": "eslint src/*.js"
  },
  "bugs": {
    "url": "https://github.com/dustinrohde/ecoscript/issues"
  },
  "homepage": "https://github.com/dustinrohde/ecoscript#readme",
  "keywords": [
    "ecosystem",
    "game",
    "simulator",
    "simulation",
    "sandbox"
  ]
}
