{
  "name": "ecos",
  "version": "0.0.1",
  "description": "Ecosystem simulator",
  "repository": {
    "type": "git",
    "url": "https://github.com/dustinrohde/ecosystem"
  },
  "main": "main.js",
  "directories": {
    "test": "test"
  },
  "author": "Dustin Rohde",
  "license": "ISC",
  "devDependencies": {
    "eslint": "2.4.0"
  },
  "dependencies": {
    "babel-cli": "6.5.1",
    "babel-plugin-transform-object-rest-spread": "6.5.0",
    "babel-polyfill": "6.5.0",
    "babel-preset-es2015": "6.5.0",
    "easystarjs": "0.2.3",
    "js-yaml": "3.5.3",
    "lodash": "4.3.0",
    "stampit": "2.1.1"
  },
  "babel": {
    "plugins": [
      "transform-object-rest-spread"
    ],
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "build": "babel src/ -o dist/",
    "watch": "babel src/ -o dist/ --watch",
    "prewatch": "npm run build",
    "lint": "eslint main.js src/*.js"
  }
}
